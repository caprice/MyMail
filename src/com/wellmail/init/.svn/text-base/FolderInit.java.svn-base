package com.wellmail.init;

import com.wellmail.model.Folder;
import com.wellmail.model.Users;

public class FolderInit {

	public Folder initFolder(Folder folder,String foldername,Users user) {
		
		folder.setFoldername(foldername);
		folder.setFolderspace(0);
		folder.setMailcount(0);
		folder.setUser(user);
		
		return folder;
	}
}
